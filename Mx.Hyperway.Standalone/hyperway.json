{
  "defaults": {
    "lookup": {
      "locator": {
        "hostname": "acc.edelivery.tech.ec.europa.eu",
        // "sml": "hyperway-sml.sediva.it",
        "bdxl": {
          "prefix": "",
          "algorithm": "SHA-256",
          "encoding": "base32"
        },
        "busdox": {
          "prefix": "B-",
          "algorithm": "MD5"
        }
      },
      "fetcher": {
        "timeouts": 10000
      }
    },
    "transport": {
      "as2_sha1": {
        "profile": "busdox-transport-as2-ver1p0",
        "sender": "hyperway-as2",
        "weight": 1000
      },
      "as2_sha512": {
        "profile": "busdox-transport-as2-ver1p0r1",
        "sender": "hyperway-as2",
        "weight": 2000
      }
    },
    "security": {
      "pki": "pki-peppol-test.xml",
      "validator": {
        "ALL": "default",
        "SMP": "SMP",
        "AP": "AP"
      } 
    },
    "hyperway": {
      "keystore": {
        "path": "./conf/test-keystore.pfx",
        "password": "test",
        "key": {
          "alias": "ap",
          "password": "test"
        }
      }
    }
  },
  "PRODUCTION": {
    "lookup": {
      "locator": {
        "hostname": "edelivery.tech.ec.europa.eu"
      }
    }
  },
  "TEST": {
    "lookup": {
      "locator": {
        "hostname": "acc.edelivery.tech.ec.europa.eu"
      }
    }
  }
}